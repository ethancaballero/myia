[run]
omit =
    conftest.py
    setup.py
    tests/*
    debug/*
    myia/debug/*
    examples/*
    myia/frontends/pytorch_functions.py

[report]
exclude_lines =
    @pyoverload
    @abstractmethod
    @compile
    @core
    @register_augm
    @register_bprop
    pragma: no cover
    AssertionError
    NotImplementedError
